# Copy this file to .env and fill in your Railway PostgreSQL credentials
# Primary discrete variables (preferred for internal + proxy fallback)
PGHOST=
PGPORT=5432
PGDATABASE=
PGUSER=
PGPASSWORD=

# Railway TCP proxy (public) fallback
RAILWAY_TCP_PROXY_DOMAIN=
RAILWAY_TCP_PROXY_PORT=5432

# Single connection string alternatives (any one works if filled)
DATABASE_URL=
RAILWAY_DATABASE_URL=
POSTGRES_URL=
POSTGRES_PRISMA_URL=
